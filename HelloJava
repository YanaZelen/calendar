public class Main {
    public static void main(String args[]) {
        int var1;
        float var2;
        double var3;
        boolean var4;
        char var5;

        var1 = 1024;
        var2 = 3.14f;
        var3 = 4.125845;
        var4 = true;
        var5 = 'J';

        System.out.println(var1 + " числовой целый тип данных int");
        System.out.println(var2 + " числовой вещественный тип данных float");
        System.out.println(var3 + " числовой вещественный тип данных double");
        System.out.println(var4 + " логический тип данных boolean");
        System.out.println(var5 + " символьный тип данных char");
    }
}
